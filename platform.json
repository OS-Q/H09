      "version": "~1.1101.0"
    },
    "framework-arduinonordicnrf5": {
      "type": "framework",
      "optional": true,
      "version": "~1.600.0"
    },
    "toolchain-gccarmnoneeabi": {
      "type": "toolchain",
      "version": ">=1.70000.0,<1.80000.0"
    },
    "tool-sreccat": {
      "version": "~1.164.0"
    },
    "tool-openocd": {
      "type": "uploader",
      "optional": true,
      "version": "~2.1000.0"
    },
    "tool-nrfjprog": {
      "type": "uploader",
      "optional": true,
      "version": "~1.90702.0"
    },
    "tool-jlink": {
      "type": "uploader",
      "optional": true,
      "version": "^1.63208.0"
    }
  }
